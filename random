package com.cts.employeeasset.userservice.repository;

import com.cts.employeeasset.userservice.model.Employee;
import org.springframework.data.jpa.repository.JpaRepository;

import java.util.List;
import java.util.Optional;

public interface EmployeeRepository extends JpaRepository<Employee, Integer> {
    Optional<Employee> findByEmpEmail(String email);
    List<Employee> findByEmpNameContainingIgnoreCase(String name);
}
